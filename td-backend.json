{
  "family": "aiqly-backend",
  "networkMode": "awsvpc",
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "1024",
  "memory": "3072",
  "executionRoleArn": "arn:aws:iam::904368995258:role/ecsTaskExecutionRole",
  "taskRoleArn": "arn:aws:iam::904368995258:role/ecsTaskExecutionRole",
  "containerDefinitions": [
    {
      "name": "backend",
      "image": "IMAGE_PLACEHOLDER",
      "cpu": 0,
      "memoryReservation": 512,
      "portMappings": [
        {
          "name": "backend-8000-tcp",
          "containerPort": 8000,
          "hostPort": 8000,
          "protocol": "tcp",
          "appProtocol": "http"
        }
      ],
      "essential": true,
      "environment": [
        { "name": "FLASK_APP", "value": "backend.main" },
        { "name": "SLACK_REDIRECT_URI", "value": "https://app.aiqly.co/slack/oauth/callback" },
        { "name": "SLACK_SCOPES", "value": "app_mentions:read,chat:write" },
        { "name": "SESSION_COOKIE_SAMESITE", "value": "None" },
        { "name": "FLASK_ENV", "value": "production" },
        { "name": "LOG_LEVEL", "value": "DEBUG" },
        { "name": "GDRIVE_WEBHOOK_URL", "value": "https://app.aiqly.co/webhook/drive" },
        { "name": "SESSION_COOKIE_SECURE", "value": "True" }
      ],
      "secrets": [
        { "name": "DATABASE_URL", "valueFrom": "arn:aws:secretsmanager:ap-northeast-1:904368995258:secret:aiqly/prod/app-go9G4u:DATABASE_URL::" },
        { "name": "FLASK_SECRET_KEY", "valueFrom": "arn:aws:secretsmanager:ap-northeast-1:904368995258:secret:aiqly/prod/app-go9G4u:FLASK_SECRET_KEY::" },
        { "name": "GOOGLE_OAUTH_CLIENT_ID", "valueFrom": "arn:aws:secretsmanager:ap-northeast-1:904368995258:secret:aiqly/prod/app-go9G4u:GOOGLE_OAUTH_CLIENT_ID::" },
        { "name": "GOOGLE_OAUTH_CLIENT_SECRET", "valueFrom": "arn:aws:secretsmanager:ap-northeast-1:904368995258:secret:aiqly/prod/app-go9G4u:GOOGLE_OAUTH_CLIENT_SECRET::" },
        { "name": "OPENAI_API_KEY", "valueFrom": "arn:aws:secretsmanager:ap-northeast-1:904368995258:secret:aiqly/prod/app-go9G4u:OPENAI_API_KEY::" },
        { "name": "REDIS_URL", "valueFrom": "arn:aws:secretsmanager:ap-northeast-1:904368995258:secret:aiqly/prod/app-go9G4u:REDIS_URL::" },
        { "name": "SLACK_BOT_TOKEN", "valueFrom": "arn:aws:secretsmanager:ap-northeast-1:904368995258:secret:aiqly/prod/app-go9G4u:SLACK_BOT_TOKEN::" },
        { "name": "SLACK_CLIENT_ID", "valueFrom": "arn:aws:secretsmanager:ap-northeast-1:904368995258:secret:aiqly/prod/app-go9G4u:SLACK_CLIENT_ID::" },
        { "name": "SLACK_CLIENT_SECRET", "valueFrom": "arn:aws:secretsmanager:ap-northeast-1:904368995258:secret:aiqly/prod/app-go9G4u:SLACK_CLIENT_SECRET::" },
        { "name": "SLACK_SIGNING_SECRET", "valueFrom": "arn:aws:secretsmanager:ap-northeast-1:904368995258:secret:aiqly/prod/app-go9G4u:SLACK_SIGNING_SECRET::" },
        { "name": "FERNET_KEY", "valueFrom": "arn:aws:secretsmanager:ap-northeast-1:904368995258:secret:aiqly/prod/app-go9G4u:FERNET_KEY::" },
        { "name": "JWT_SECRET_KEY", "valueFrom": "arn:aws:secretsmanager:ap-northeast-1:904368995258:secret:aiqly/prod/app-go9G4u:JWT_SECRET_KEY::" }
      ],
      "readonlyRootFilesystem": false,
      "ulimits": [],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/aiqly-backend",
          "mode": "non-blocking",
          "awslogs-create-group": "true",
          "max-buffer-size": "25m",
          "awslogs-region": "ap-northeast-1",
          "awslogs-stream-prefix": "ecs"
        }
      },
      "systemControls": []
    }
  ],
  "volumes": [],
  "placementConstraints": [],
  "runtimePlatform": {
    "cpuArchitecture": "ARM64",
    "operatingSystemFamily": "LINUX"
  },
  "enableFaultInjection": false
}
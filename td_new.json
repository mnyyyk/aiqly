{
  "containerDefinitions": [
    {
      "name": "backend",
      "image": "904368995258.dkr.ecr.ap-northeast-1.amazonaws.com/chachat-backend:latest",
      "cpu": 0,
      "memoryReservation": 512,
      "portMappings": [
        {
          "containerPort": 8000,
          "hostPort": 8000,
          "protocol": "tcp",
          "name": "backend-8000-tcp",
          "appProtocol": "http"
        }
      ],
      "essential": true,
      "environment": [
        {
          "name": "FLASK_APP",
          "value": "backend.main"
        },
        {
          "name": "SLACK_REDIRECT_URI",
          "value": "https://app.aiqly.co/slack/oauth/callback"
        },
        {
          "name": "SLACK_SCOPES",
          "value": "app_mentions:read,chat:write"
        },
        {
          "name": "SESSION_COOKIE_SAMESITE",
          "value": "None"
        },
        {
          "name": "FLASK_ENV",
          "value": "production"
        },
        {
          "name": "GDRIVE_WEBHOOK_URL",
          "value": "https://app.aiqly.co/webhook/drive"
        },
        {
          "name": "SESSION_COOKIE_SECURE",
          "value": "True"
        }
      ],
      "environmentFiles": [],
      "mountPoints": [],
      "volumesFrom": [],
      "secrets": [
        {
          "name": "DATABASE_URL",
          "valueFrom": "arn:aws:secretsmanager:ap-northeast-1:904368995258:secret:aiqly/prod/app-go9G4u:DATABASE_URL::"
        },
        {
          "name": "FLASK_SECRET_KEY",
          "valueFrom": "arn:aws:secretsmanager:ap-northeast-1:904368995258:secret:aiqly/prod/app-go9G4u:FLASK_SECRET_KEY::"
        },
        {
          "name": "GOOGLE_OAUTH_CLIENT_ID",
          "valueFrom": "arn:aws:secretsmanager:ap-northeast-1:904368995258:secret:aiqly/prod/app-go9G4u:GOOGLE_OAUTH_CLIENT_ID::"
        },
        {
          "name": "GOOGLE_OAUTH_CLIENT_SECRET",
          "valueFrom": "arn:aws:secretsmanager:ap-northeast-1:904368995258:secret:aiqly/prod/app-go9G4u:GOOGLE_OAUTH_CLIENT_SECRET::"
        },
        {
          "name": "OPENAI_API_KEY",
          "valueFrom": "arn:aws:secretsmanager:ap-northeast-1:904368995258:secret:aiqly/prod/app-go9G4u:OPENAI_API_KEY::"
        },
        {
          "name": "SLACK_CLIENT_ID",
          "valueFrom": "arn:aws:secretsmanager:ap-northeast-1:904368995258:secret:aiqly/prod/app-go9G4u:SLACK_CLIENT_ID::"
        },
        {
          "name": "SLACK_CLIENT_SECRET",
          "valueFrom": "arn:aws:secretsmanager:ap-northeast-1:904368995258:secret:aiqly/prod/app-go9G4u:SLACK_CLIENT_SECRET::"
        },
        {
          "name": "SLACK_SIGNING_SECRET",
          "valueFrom": "arn:aws:secretsmanager:ap-northeast-1:904368995258:secret:aiqly/prod/app-go9G4u:SLACK_SIGNING_SECRET::"
        }
      ],
      "readonlyRootFilesystem": false,
      "ulimits": [],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/aiqly-backend",
          "mode": "non-blocking",
          "awslogs-create-group": "true",
          "max-buffer-size": "25m",
          "awslogs-region": "ap-northeast-1",
          "awslogs-stream-prefix": "ecs"
        },
        "secretOptions": []
      },
      "systemControls": []
    }
  ],
  "family": "aiqly-backend",
  "taskRoleArn": "arn:aws:iam::904368995258:role/ecsTaskExecutionRole",
  "executionRoleArn": "arn:aws:iam::904368995258:role/ecsTaskExecutionRole",
  "networkMode": "awsvpc",
  "volumes": [],
  "placementConstraints": [],
  "runtimePlatform": {
    "cpuArchitecture": "ARM64",
    "operatingSystemFamily": "LINUX"
  },
  "requiresCompatibilities": [
    "FARGATE"
  ],
  "cpu": "1024",
  "memory": "3072",
  "enableFaultInjection": false
}
